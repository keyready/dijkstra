{
  "name": "dijkstra",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "init": "concurrently -n client-init,server-init \"npm run client:init\" \"npm run server:init\"",
    "start:dev": "concurrently -n client,server \"npm run dev\" \"nodemon server/server.js\"",
    "client:init": "npm install",
    "server:init": "cd ./server && npm install"
  },
  "dependencies": {
    "concurrently": "^8.2.0",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.5",
    "next": "13.4.5",
    "nodemon": "^2.0.22",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.63.4"
  }
}
